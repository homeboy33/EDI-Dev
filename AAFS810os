}

Initialization { 
	[]
	;;
        VAR->SAPEDI40 = "Yes"
        VAR->Global10 = "Y"
	SET_FIRST_DELIM(13)
	;;
	VAR->CTT01 = 0
	VAR->TDS01 = 0
	VAR->TDS02 = 0

	;;
	[]
	VAR->SourceStatus = 1

	;;
	;;
}*0 .. 1 ;; |-- end Initialization --|


Document_Loop { 
EDI_DC40 { LineFeedDelimRecord  "EDI_DC40  "
MANDT { AlphaNumericFld @1 .. 3  none 
}*0 .. 1 ;; |-- end MANDT --|
DOCNUM { AlphaNumericFld @1 .. 16  none 
}*0 .. 1 ;; |-- end DOCNUM --|
