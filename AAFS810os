}*0 .. 1 ;; |-- end CREDAT --|
CRETIM { AlphaNumericFld @1 .. 6  none 
}*0 .. 1 ;; |-- end CRETIM --|
REFINT { AlphaNumericFld @1 .. 14  none 
}*0 .. 1 ;; |-- end REFINT --|
REFGRP { AlphaNumericFld @1 .. 14  none 
}*0 .. 1 ;; |-- end REFGRP --|
REFMES { AlphaNumericFld @1 .. 14  none 
}*0 .. 1 ;; |-- end REFMES --|
ARCKEY { AlphaNumericFld @1 .. 70  none 
}*0 .. 1 ;; |-- end ARCKEY --|
SERIAL { AlphaNumericFld @1 .. 20  none 
}*0 .. 1 ;; |-- end SERIAL --|

[]
PERFORM ("SPStatusSetup", &DOCNUM, &STATUS, &IDOCTYP, &MANDT)

	;;
	;; DEBUG PURPOSES!!!
	;;
	[]
	;; SEND_SMSG(1,"#+")
	;; SEND_SMSG(1,STRCATM(5,"PROCESSING ",STRCAT(STRTRIM(RCVPRN,"T"," "),STRCAT("~",STRTRIM(STDMES,"T"," ")))," ","IDOC# ",DOCNUM))


	;;
	;;***********************************
	;; RETRIEVE THE APPLICATION ID
	;;***********************************
	;;
	[]
	SET_EVAR("XREF_KEY","ENTITY") 

	VAR->OTRecognID= STRCAT(STRTRIM(RCVPRN,"T"," "),STRCAT("~",STRTRIM(STDMES,"T"," "))) 

	VAR->XRefSts   = XREF( "ENTITY" , VAR->OTRecognID , &VAR->OTHierarchyID , "Y" ) 

	SET_EVAR("HIERARCHY_KEY",STRSUBS(VAR->OTHierarchyID,01,16)) 

	;;
	;;****************************************
	;; APPLICATION X-REFERENCE DOENS'T EXISTS
	;;****************************************
	;;
	[VAR->XRefSts != 0 ] EXIT 501 
	;;
	;;
}*1 .. 1 ;; |-- end EDI_DC40 --|



