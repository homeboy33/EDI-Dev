}*0 .. 1 ;; |-- end MESFCT --|
STD { AlphaNumericFld @1 .. 1  none 
}*0 .. 1 ;; |-- end STD --|
STDVRS { AlphaNumericFld @1 .. 6  none 
}*0 .. 1 ;; |-- end STDVRS --|
STDMES { AlphaNumericFld @1 .. 6  none 
}*0 .. 1 ;; |-- end STDMES --|
SNDPOR { AlphaNumericFld @1 .. 10  none 
}*0 .. 1 ;; |-- end SNDPOR --|
SNDPRT { AlphaNumericFld @1 .. 2  none 
}*0 .. 1 ;; |-- end SNDPRT --|
SNDPFC { AlphaNumericFld @1 .. 2  none 
}*0 .. 1 ;; |-- end SNDPFC --|
SNDPRN { AlphaNumericFld @1 .. 10  none 
}*0 .. 1 ;; |-- end SNDPRN --|
SNDSAD { AlphaNumericFld @1 .. 21  none 
}*0 .. 1 ;; |-- end SNDSAD --|
SNDLAD { AlphaNumericFld @1 .. 70  none 
}*0 .. 1 ;; |-- end SNDLAD --|
RCVPOR { AlphaNumericFld @1 .. 10  none 
}*0 .. 1 ;; |-- end RCVPOR --|
RCVPRT { AlphaNumericFld @1 .. 2  none 
}*0 .. 1 ;; |-- end RCVPRT --|
RCVPFC { AlphaNumericFld @1 .. 2  none 
}*0 .. 1 ;; |-- end RCVPFC --|
RCVPRN { AlphaNumericFld @1 .. 10  none 
}*0 .. 1 ;; |-- end RCVPRN --|
RCVSAD { AlphaNumericFld @1 .. 21  none 
}*0 .. 1 ;; |-- end RCVSAD --|
RCVLAD { AlphaNumericFld @1 .. 70  none 
}*0 .. 1 ;; |-- end RCVLAD --|
